    "js-base64": "^3.7.7",
    "pinia": "^2.3.0",
    "pinia-plugin-persistedstate": "^3.2.3",
    "sass": "^1.82.0",
    "vue": "^3.5.13",
    "vue-cookies": "^1.8.4",
    "vue-echarts": "^6.7.3",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node18": "^2.0.1",
