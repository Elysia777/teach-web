    "eslint": "^9.16.0",
    "eslint-plugin-vue": "^9.32.0",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "typescript": "~5.0.4",
    "vite": "^4.5.5",
    "vitest": "^0.32.4",
    "vue-tsc": "^1.8.27"
